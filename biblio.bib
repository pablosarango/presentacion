	urldate = {2015-11-23},
	publisher = {Universitat Oberta La Salle},
	author = {Lopez-Pellicer, Francisco J. and Béjar, Rubén and Latre, Miguel A. and Nogueras-Iso, Javier and Zarazaga-Soria, Francisco Javier},
	month = jul,
	year = {2015},
